{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "FormatterName": "systemd",
      "FormatterOptions": {
        "IncludeScopes": false,
        "UseUtcTimestamp": false
      }
    }
  },
  "InitialState": {
    "Root": {
      "serialNumber": "1234567890.json"
    },
    "thermostat1": {
      "targetTemperature": 1001.0
    },
    "thermostat2": {
      "targetTemperature": 2002.0
    },
    "deviceInformation": {
      "manufacturer": "Example.json",
      "model": "Thermostat Controller.json"
    }
  }
}
